<%- include('../partials/header.ejs', {user}) %>

<div class="m-3">
    <h2 class="p-2">All Projects</h2>
</div>

<div class="list-group m-3">
    <% project.forEach(project => { %>
        <a href="/projects/<%=project.id %>" class="list-group-item list-group-item-action" aria-current="true">
            <div class="d-flex w-30 justify-content-start">
            <h4 class="mb-1">project: <%= project.name %></h4>
        </div>
            <p class="mb-1"> <span class="h5">Due Date:</span> <%= project.due_date %></p>
            <p class="mb-1"> <span class="h5">project Progress: </span> <%= project.complete %></p>
        </a>
    <% }) %>
</div>
<%- include('../partials/footer.ejs') %>